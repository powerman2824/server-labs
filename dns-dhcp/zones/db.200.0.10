$TTL	1w
@	IN	SOA	dns.example.com. root.dns.example.com. (
			3		; Serial
			1w 		; Refresh
			1d		; Retry
			28d		; Expire
			1w) 		; Negative Cache TTL
			 
; name servers - NS records
		IN	NS	dns.example.com.

; PTR Records
2.200.0.10.in-addr.arpa. 	IN	PTR	dns.example.com.
1.200.0.10.in-addr.arpa.        IN      PTR     router1.example.com.
